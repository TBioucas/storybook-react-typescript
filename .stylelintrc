{
  "plugins": [
    "stylelint-prettier"
  ],
  "rules": {
    "prettier/prettier": true,
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "function",
          "if",
          "each",
          "include",
          "mixin",
          "return"
        ]
      }
    ]
  },
  "extends": [
    "stylelint-config-standard",
    "stylelint-config-prettier"
  ]
}
